<!DOCTYPE html>
<html ng-app="myApp">
<head>
    <!--<link rel="stylesheet" href="/libs/bootstrap/css/bootstrap.css"/>-->
    <link rel="stylesheet" href="/stylesheets/screen.css"/>
    <title></title>
</head>
<body>
<div class="container" ng-controller="AppCtrl" id="main-content">
    <div class="logo-wrap">
        <div class="logo-center">
            <img src="/assets/img/logo-100.png"/>
        </div>
    </div>
    <div id="content-wrap">
        <ng-view></ng-view>
        <img src="/assets/img/interface.png" id="interface-img"/>

        <div class="controls-wrap" ng-show="appModel.currentItem">
            <p class="comp-title">{{appModel.currentItem.name}}</p>
            <img src="/assets/img/left-arrow.png" class="left-arrow"/>
            <a ng-click="onNext()">
                <img src="/assets/img/right-arrow.png" class="right-arrow"/>
            </a>
            <div class="arrow-center">
                <a ng-click="onBack()">
                    <img src="/assets/img/down-arrow.png"/>
                </a>
            </div>
        </div>

        <div class="layout-wrap" ng-show="!appModel.currentItem">
            <a ng-click="onDisk()">
                <img src="/assets/img/disk.png" class="left-arrow"/>
            </a>
            <a ng-click="onGrid()">
                <img src="/assets/img/grid.png" class="right-arrow"/>
            </a>
            <div class="sphere-btn">
                <a ng-click="onSphere()">
                    <img src="/assets/img/sphere.png"/>
                </a>
            </div>
        </div>


    </div>

</div>
<script src="/libs/jquery-1.8.2.min.js"></script>
<script src="/libs/three/three.js"></script>
<script src="/libs/three/tween.js"></script>
<script src="/libs/three/TrackballControls.js"></script>
<script src="/libs/three/CSS3DRenderer.js"></script>
<script src="/libs/bootstrap/js/bootstrap.js"></script>
<script src="/libs/angular-1.0.2/angular.js"></script>
<script src="/js/app.js"></script>
<script src="/js/services.js"></script>
<script src="/js/controllers.js"></script>
<script src="/js/filters.js"></script>
<script src="/js/directives.js"></script>
</body>
</html>